concept
	[at0000]	-- Barthel index
description
	author = <"Sam Heard <sam.heard@oceaninformatics.biz>">
	submission = <
		organisation = <"openEHR Foundation">
		date = <2004-05-18>
	>
		version = <"version">
	status = <"draft">
	revision = <"1.0">
	description("en") = <
		purpose = <"Describe systemic blood pressure measurement result and protocol">
		use = <"">
		misuse = <"">
	>
	adl_version = <"1.2">
	rights = <"">

definition
	OBSERVATION[at0000] matches {	-- Barthel index
		data matches {
			HISTORY[at0002] matches {	-- history
				events cardinality matches {1..*} matches {
					EVENT[at0003] occurrences matches {0..*} matches {	-- Any event
						data matches {
							LIST[at0001] matches {	-- structure
								items cardinality matches {0..1} matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Bowels
										value matches {
											ORDINAL matches {
												value matches {
													0|[local::at0005], 	-- Incontinent
													1|[local::at0006], 	-- Sometimes incontinent
													2|[local::at0007]  	-- Continent
												}
											}
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {	-- Urine
										value matches {
											ORDINAL matches {
												value matches {
													0|[local::at0005], 	-- Incontinent
													1|[local::at0006], 	-- Sometimes incontinent
													2|[local::at0007]  	-- Continent
												}
											}
										}
									}
									ELEMENT[at0009] occurrences matches {0..1} matches {	-- Bathing/Show
										value matches {
											ORDINAL matches {
												value matches {
													0|[local::at0010], 	-- Dependent
													1|[local::at0011]  	-- Independent
												}
											}
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {	-- Dressing
										value matches {
											ORDINAL matches {
												value matches {
													0|[local::at0013], 	-- Dependent
													1|[local::at0014], 	-- 50% - needs assistance
													2|[local::at0011]  	-- Independent
												}
											}
										}
									}
									ELEMENT[at0016] occurrences matches {0..1} matches {	-- Total
										value matches {
											COUNT matches {
												magnitude matches {|0..20|}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

