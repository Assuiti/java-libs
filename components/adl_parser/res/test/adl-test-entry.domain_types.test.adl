concept
	[at0000]	-- HIV quality registry
description
	details = <
		["en"] = <
			language = <"en">
			purpose = <"parser test for domain data types">
			keywords = <"data types">
			copyright = <"copyright (c) 2004 ACODE HB">
		>
	>
	lifecycle_state = <"draft">
	original_author = <
		["name"] = <"Rong Chen">
		["organisation"] = <"ACODE HB">
		["date"] = <"2004-10-05">
	>

definition
	INSTRUCTION[at0000] matches {	-- HIV medication order
        action matches {
            LIST[at0001] matches {
                items cardinality matches {0..*} matches {
                    ELEMENT[at10001] matches {	-- ordinal
                        value matches {
                            0|[local::at0003.0], 	-- capsule
                            1|[local::at0003.1], 	-- powder
                            2|[local::at0003.2],  	-- tablet
                            3|[local::at0003.3],  	-- solution
                            4|[local::at0003.4]  	-- suspension
                        }
                    }
                    ELEMENT[at10002] matches {	-- coded_text
                        value matches {
                            [local::
                            at2001.0, 	-- initial
                            at2001.1, 	-- wait
                            at2001.2] 	-- completed
                        }
                    }
                    ELEMENT[at10003] matches {	-- quantity
                        value matches {
                            QUANTITY matches {
                                magnitude matches {|10.0..100.0|}
                                units matches {"mg"}
                            }
                        }
                    }
                    ELEMENT[at10004] matches {	-- count
                        value matches {
                            COUNT matches {
                                magnitude matches {|1..8|}
                            }
                        }
                    }
                }
            }
        }
	}

