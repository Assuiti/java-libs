archetype 
	openEHR-EHR-SECTION.post_natal_exam.draft
concept 
	[at0000]	-- post-natal examination headings

description
	author = 		<"Sam Heard <s.heard@littlerock.com>">
	submission = 	<
		organisation = <"World Health Organisation">
		date = <2003-06-10>
	>
	version = 		<"draft">
	status = 		<"draft">
	revision = 		<"1.0">
	description("en") =	<
		purpose = 		<"Headings for post-natal examination">
		use = 		<"">
		misuse = 		<"">
	>
	adl_version =	<"0.9">
	rights = 		<"">

definition
	SECTION[at0001] matches {						-- Post-natal Examination
		name matches {
			CODED_TEXT matches {
				code matches {[local::at0000]}
			}
		}
		items cardinality matches {0..*} matches {			-- allow for multiple babies
			SECTION[at1000] occurrences matches {0..*}  matches {	-- Neonate
				name matches {
					CODED_TEXT matches {
						code matches {[local::at1000]}
					}
				}
				items cardinality matches {0..*} matches {
					allow_archetype OBSERVATION occurrences matches {0..*} matches {		-- any OBSERVATION 
						include
							id matches {"openehr-ehr-observation.*.*"}
					}
				}
			}

			SECTION[at2000] occurrences matches {0..1}  matches {	-- mother
				name matches {
					CODED_TEXT matches {
						code matches {[local::at2000]}
					}
				}
				items cardinality matches {0..*} matches {
					allow_archetype OBSERVATION occurrences matches {0..*} matches {			-- any OBSERVATION
						include
							id matches {"openehr-ehr-observation.*.*"}
					}
				}
			}
		}
	}


