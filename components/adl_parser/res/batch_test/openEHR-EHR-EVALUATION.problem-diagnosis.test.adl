archetype
	openEHR-EHR-EVALUATION.problem-diagnosis.test
specialize
	openEHR-EHR-EVALUATION.problem.test

concept
	[at0000.1]	-- Diagnosis as defined by a clinician
description
	author = <"Sam Heard <sam.heard@oceaninformatics.biz>">
	submission = <
		organisation = <"openEHR Foundation">
		date = <2004-05-18>
	>
		version = <"version">
	status = <"draft">
	revision = <"1.0">
	description("en") = <
		purpose = <"Describe systemic blood pressure measurement result and protocol">
		use = <"">
		misuse = <"">
	>
	adl_version = <"1.2">
	rights = <"">

definition
	EVALUATION[at0000.1] matches {	-- Diagnosis as defined by a clinician
		data matches {
			TREE[at0002] matches {	-- structure
				items cardinality matches {0..1} matches {
					ELEMENT[at0003.1] matches {	-- Diagnosis
						runtime_label matches {
							CODED_TEXT matches {
								code matches {[ac0001.1]}		-- =Diagnosis
							}
						}
						value matches {
							CODED_TEXT matches {
								code matches {[ac0002]}		-- Any term that 'is_a' diagnosis
							}
						}
					}
					ELEMENT[at0.22] occurrences matches {0..1} matches {	-- Status
						value matches {
							CODED_TEXT matches {
								code matches {
									[local::
									at0.23, 	-- Provisional
									at0.24, 	-- Working
									at0.25]	-- Final
								}
							}
						}
					}
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Date of onset
						runtime_label matches {
							CODED_TEXT matches {
								code matches {[ac0004]}		-- =Date of onset
							}
						}
						value matches {yyyy-??-??}
					}
					ELEMENT[at0005] occurrences matches {0..1} matches {	-- Age at onset
						runtime_label matches {
							CODED_TEXT matches {
								code matches {[ac0003]}		-- =Age at onset
							}
						}
						value matches {
							REAL_QUANTITY matches {
								property matches {"time"}
								units cardinality matches {1} matches {
									UNIT matches {
										unit_string matches {"yr"}
										magnitude matches {|0.0..200.0|}
									}
								}
							}
						}
					}
					ELEMENT[at0007] occurrences matches {0..1} matches {	-- Number of occurrences
						runtime_label matches {
							CODED_TEXT matches {
								code matches {[ac0006]}		-- =Number of occurrences
							}
						}
						value matches {
							COUNT matches {
								magnitude matches {|>= 1|}
							}
						}
					}
					ELEMENT[at0025] occurrences matches {0..1} matches {	-- Severity
						value matches {
							ORDINAL matches {
								value matches {
									0|[local::at0026], 	-- Mild
									5|[local::at0027], 	-- Moderate
									10|[local::at0028]  	-- Severe
								}
							}
						}
					}
					ELEMENT[at0008] occurrences matches {0..1} matches {	-- Clinical description
						runtime_label matches {
							CODED_TEXT matches {
								code matches {[ac0005]}		-- =Description
							}
						}
						value matches {
							TEXT matches {*}
						}
					}
					ELEMENT[at0009] occurrences matches {0..1} matches {	-- Date clinically recognised
						runtime_label matches {
							CODED_TEXT matches {
								code matches {[ac0007]}		-- =Date of diagnosis
							}
						}
						value matches {yyyy-??-??}
					}
					CLUSTER[at0010] occurrences matches {0..*} matches {	-- Location
						runtime_label matches {
							CODED_TEXT matches {
								code matches {[ac0008]}		-- =Location
							}
						}
						items cardinality matches {0..1} matches {
							ELEMENT[at0011] occurrences matches {0..1} matches {	-- Body site
								runtime_label matches {
									CODED_TEXT matches {
										code matches {[ac0009]}		-- =Body site
									}
								}
								value matches {
									CODED_TEXT matches {
										code matches {[ac0010]}		-- Any term that describes a body site
									}
								}
							}
							ELEMENT[at0012] occurrences matches {0..1} matches {	-- Location description
								runtime_label matches {
									CODED_TEXT matches {
										code matches {[ac0011]}		-- =Description
									}
								}
								value matches {
									TEXT matches {*}
								}
							}
						}
					}
					CLUSTER[at0013] occurrences matches {0..1} matches {	-- Complications
						items cardinality matches {0..1} matches {
							ELEMENT[at0014] occurrences matches {0..*} matches {	-- Complication
								runtime_label matches {
									CODED_TEXT matches {
										code matches {[ac0012]}		-- =Complication
									}
								}
								value matches {
									CODED_TEXT matches {
										code matches {[ac0013]}		-- Any term that 'is_a' problem
									}
								}
							}
						}
					}
					ELEMENT[at0015] occurrences matches {0..1} matches {	-- Date of resolution/remission
						runtime_label matches {
							CODED_TEXT matches {
								code matches {[ac0014]}		-- =Date of resolution/remission
							}
						}
						value matches {yyyy-??-??}
					}
					ELEMENT[at0029] occurrences matches {0..1} matches {	-- Age at resolution
						value matches {
							REAL_QUANTITY matches {
								property matches {"time"}
								units cardinality matches {1} matches {
									UNIT matches {
										unit_string matches {"yr"}
										magnitude matches {|0.0..200.0|}
									}
								}
							}
						}
					}
					CLUSTER[at0.2] occurrences matches {0..1} matches {	-- Diagnostic criteria
						items cardinality matches {0..1} matches {
							ELEMENT[at0.3] occurrences matches {0..*} matches {	-- Criterion
								runtime_label matches {
									CODED_TEXT matches {
										code matches {[ac0.2]}		-- =Diagnostic criterion
									}
								}
								value matches {
									TEXT matches {*}
								}
							}
						}
					}
					CLUSTER[at0.4] occurrences matches {0..1} matches {	-- Clinical staging
						runtime_label matches {
							CODED_TEXT matches {
								code matches {[ac0.3]}		-- =Clinical staging
							}
						}
						items cardinality matches {0..1} matches {
							ELEMENT[at0.5] occurrences matches {0..1} matches {	-- Stage
								runtime_label matches {
									CODED_TEXT matches {
										code matches {[ac0.4]}		-- =Stage
									}
								}
								value matches {
									CODED_TEXT matches {
										code matches {[ac0.5]}		-- Any term that describes the stage of a diagnosis
									}
								}
							}
							ELEMENT[at0.6] occurrences matches {0..1} matches {	-- Tumour
								runtime_label matches {
									CODED_TEXT matches {
										code matches {[ac0.6]}		-- =Tumor
									}
								}
								value matches {
									ORDINAL matches {
										value matches {
											-1|[local::at0.7], 	-- Tx - Primary tumor not assessed
											0|[local::at0.8], 	-- T0 - no primary tumor
											5|[local::at0.9], 	-- Tis - Carcinoma insitu
											10|[local::at0.10], 	-- T1 - Micro-invasion 0.5 cm
											20|[local::at0.11], 	-- T2 - Tumor invading 1cm
											30|[local::at0.12], 	-- T3 - Full thickness of structure
											40|[local::at0.13]  	-- T4 - Invading adjacent structure
										}
									}
								}
							}
							ELEMENT[at0.14] occurrences matches {0..1} matches {	-- Nodes
								runtime_label matches {
									CODED_TEXT matches {
										code matches {[ac0.8]}		-- =Nodes
									}
								}
								value matches {
									ORDINAL matches {
										value matches {
											-1|[local::at0.15], 	-- Nx - regional nodes not assessed
											0|[local::at0.16], 	-- N0 - no regional nodes involved
											10|[local::at0.17]  	-- N1 - Regional node involvement
										}
									}
								}
							}
							ELEMENT[at0.18] occurrences matches {0..1} matches {	-- Metastases
								runtime_label matches {
									CODED_TEXT matches {
										code matches {[ac0.7]}		-- =Matastases
									}
								}
								value matches {
									ORDINAL matches {
										value matches {
											-1|[local::at0.19], 	-- Mx - metastases not assessed
											0|[local::at0.20], 	-- M0 - no distant metastases
											10|[local::at0.21]  	-- M1 - Distant metastases
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

