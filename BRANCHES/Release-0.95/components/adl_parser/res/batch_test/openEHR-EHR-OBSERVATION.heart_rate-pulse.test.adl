definition
	OBSERVATION[at0000.1] matches {	-- Pulse
		data matches {
			HISTORY[at0002] matches {	-- history
				events cardinality matches {1..*} matches {
					EVENT[at0003] occurrences matches {0..*} matches {	-- Any event
						data matches {
							List[at0001] matches {	-- structure
								items cardinality matches {0..1} matches {
									ELEMENT[at0004] matches {	-- Rate
										value matches {
											COUNT matches {
												magnitude matches {|>= 0|}
											}
										}
									}
									ELEMENT[at0005] occurrences matches {0..1} matches {	-- Rhythm
										value matches {
											CODED_TEXT matches {
												code matches {
													[local::
													at0006, 	-- Regular
													at0007, 	-- Irregular
													at0008]	-- Irregularly irregular
												}
											}
										}
									}
									ELEMENT[at0.9] occurrences matches {0..1} matches {	-- Character
										value matches {
											CODED_TEXT matches {
												code matches {
													[local::
													at0.10, 	-- Normal
													at0.11, 	-- Bounding
													at0.12]	-- Thready
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

