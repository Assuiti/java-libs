archetype
	openEHR-EHR-OBSERVATION.Intervention-medication.test
specialize
	openEHR-EHR-OBSERVATION.Intervention.test

concept
	[at0000.1]	-- Administration of medication
description
	author = <"Sam Heard <s.heard@littlerock.com>">
	submission = <
		organisation = <"World Health Organisation">
		date = <2003-06-10>
	>
		version = <"draft">
	status = <"draft">
	revision = <"1.1">
	description("en") = <
		purpose = <"SOAP Heading structure for the problem-oriented medical record (POMR)">
		use = <"">
		misuse = <"">
	>
	adl_version = <"1.2">
	rights = <"">

definition
	OBSERVATION[at0000.1] matches {	-- Administration of medication
		data matches {
			HISTORY[at0002] matches {	-- history
				events cardinality matches {1..*} matches {
					EVENT[at0003] occurrences matches {0..*} matches {	-- Any event
						width matches {P1m}
						data matches {
							Tree[at0001] matches {	-- structure
								items cardinality matches {0..1} matches {
									ELEMENT[at0004.1] matches {	-- Name of medication
										value matches {
											TEXT matches {*}
										}
									}
									ELEMENT[at0.10] occurrences matches {0..1} matches {	-- Strength
										value matches {
											QUANTITY matches {
												units matches {"s"}
											}
										}
									}
									ELEMENT[at0.11] occurrences matches {0..1} matches {	-- Form
										value matches {
											CODED_TEXT matches {
												code matches {[ac0.1]}		-- Any term that 'is_a' form of medication
											}
										}
									}
									ELEMENT[at0.13] occurrences matches {0..1} matches {	-- Dose
										value matches {
											QUANTITY matches {
												units matches {"s"}
											}
										}
									}
									ELEMENT[at0.12] occurrences matches {0..1} matches {	-- Route
										value matches {
											CODED_TEXT matches {
												code matches {[ac0.2]}		-- Any term that 'is_a' route of administration
											}
										}
									}
									ELEMENT[at0008] occurrences matches {0..1} matches {	-- Indication
										value matches {
											TEXT matches {*}
										}
									}
									ELEMENT[at0005.1] occurrences matches {0..1} matches {	-- Patient instructions
										value matches {
											TEXT matches {*}
										}
									}
									ELEMENT[at0006] occurrences matches {0..1} matches {	-- Age
										value matches {
											QUANTITY matches {
												units matches {"s"}
											}
										}
									}
									ELEMENT[at0007] occurrences matches {0..1} matches {	-- Provider
										value matches {
											TEXT matches {*}
										}
									}
									ELEMENT[at0.9] occurrences matches {0..1} matches {	-- Alternative name
										runtime_label matches {
											CODED_TEXT matches {
												code matches {[ac0.3]}		-- =Generic name OR Brand name
											}
										}
										value matches {
											TEXT matches {*}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

