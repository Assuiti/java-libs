definition
	OBSERVATION[at0000] matches {	-- Visual observation
		data matches {
			HISTORY[at0002] matches {	-- history
				events cardinality matches {1..*} matches {
					EVENT[at0003] occurrences matches {0..*} matches {	-- Any event
						data matches {
							TREE[at0001] matches {	-- structure
								items cardinality matches {0..1} matches {
									CLUSTER[at0008] occurrences matches {1..*} matches {	-- Object and findings
										items cardinality matches {0..1} matches {
											ELEMENT[at0009] matches {	-- Finding site
												value matches {
													CODED_TEXT matches {
														code matches {[ac0001]}		-- =any body part including the whole
													}
												}
											}
											ELEMENT[at0010] occurrences matches {0..1} matches {	-- Finding site description
												value matches {
													TEXT matches {*}
												}
											}
										}
									}
									CLUSTER[at0011] occurrences matches {0..1} matches {	-- Findings
										items cardinality matches {0..1} matches {
											ELEMENT[at0013] occurrences matches {0..1} matches {	-- Normal
												value matches {True, False}
											}
											ELEMENT[at0014] occurrences matches {0..1} matches {	-- Clinical findings
												value matches {
													TEXT matches {*}
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

