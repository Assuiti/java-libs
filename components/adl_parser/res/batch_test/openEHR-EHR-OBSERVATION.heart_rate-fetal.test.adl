concept
	[at0000.1]	-- Foetal heart rate
description
	author = <"Sam Heard <sam.heard@oceaninformatics.biz>">
	submission = <
		organisation = <"openEHR Foundation">
		date = <2004-05-18>
	>
		version = <"version">
	status = <"draft">
	revision = <"1.0">
	description("en") = <
		purpose = <"Describe systemic blood pressure measurement result and protocol">
		use = <"">
		misuse = <"">
	>
	adl_version = <"1.2">
	rights = <"">

definition
	OBSERVATION[at0000.1] matches {	-- Foetal heart rate
		subject matches {
			RELATED_PARTY[at0.16] matches {	-- Subject of data
				relationship matches {
					CODED_TEXT matches {
						code matches {[openehr::3]}
						assumed_value matches {"relationship"}
					}
				}
			}
		}
		data matches {
			HISTORY[at0002] matches {	-- history
				events cardinality matches {1..*; ordered} matches {
					EVENT[at0003] occurrences matches {0..*} matches {	-- Any event
						data matches {
							List[at0001] matches {	-- structure
								items cardinality matches {0..1; ordered} matches {
									ELEMENT[at0004] matches {	-- Rate
										value matches {
											COUNT matches {
												magnitude matches {|>= 0|}
											}
										}
									}
									ELEMENT[at0005] occurrences matches {0..1} matches {	-- Rhythm
										value matches {
											CODED_TEXT matches {
												code matches {
													[local::
													at0006, 	-- Regular
													at0007, 	-- Irregular
													at0008]	-- Irregularly irregular
												}
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
		protocol matches {
			List[at0.10] matches {	-- List
				items cardinality matches {0..1; ordered} matches {
					ELEMENT[at0.11] occurrences matches {0..1} matches {	-- Device
						value matches {
							CODED_TEXT matches {
								code matches {
									[local::
									at0.12, 	-- Pinnards stethoscope
									at0.13, 	-- Doppler
									at0.14]	-- Electrical
								}
							}
						}
					}
				}
			}
		}
	}

