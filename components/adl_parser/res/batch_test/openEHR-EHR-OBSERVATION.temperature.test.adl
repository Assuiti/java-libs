definition
	OBSERVATION[at0000] matches {	-- body temperature
		data matches {
			HISTORY[at0002] matches {	-- history
				events cardinality matches {1..*} matches {
					EVENT[at0003] occurrences matches {0..*} matches {	-- Any event
						data matches {
							SIMPLE[at0001] matches {	-- Simple
								item matches {
									ELEMENT[at0004] occurrences matches {0..1} matches {	-- Temperature
										runtime_label matches {
											CODED_TEXT matches {
												code matches {[ac0001]}		-- Any synonym for 'temperature'
											}
										}
										value matches {
											REAL_QUANTITY matches {
												property matches {"temperature"}
												units cardinality matches {1} matches {
													UNIT matches {
														unit_string matches {"C"}
														magnitude matches {|>= 4|}
													}
												}
											}
										}
									}
								}
							}
						}
					}
					EVENT[at0005] occurrences matches {0..*} matches {	-- 1 hour average
						aggregate_math_function matches {"Mean"}
						width matches {P1h}
						data matches {
							use_node SIMPLE /[at0000]/data[at0002]/events[at0003]/data[at0001]/
						}
					}
					EVENT[at0006] occurrences matches {0..*} matches {	-- 1 hour maximum
						aggregate_math_function matches {"Maximum"}
