													3|[local::at0008], 	-- ++
													4|[local::at0009], 	-- +++
													5|[local::at0010]  	-- ++++
												}
											}
										}
									}
									ELEMENT[at0042] occurrences matches {0..1} matches {	-- Babinski response
										value matches {
											ORDINAL matches {
												value matches {
													0|[local::at0043], 	-- Strongly down-going
