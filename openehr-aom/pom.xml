	<modelVersion>4.0.0</modelVersion>
	<groupId>openehr</groupId>
	<artifactId>openehr-aom</artifactId>
	<packaging>jar</packaging>
	<version>0.1-SNAPSHOT</version>
	<name>openEHR Archetype Object Model</name>
	<url>http://svn.openehr.org/ref_impl_java/TRUNK/project_page.htm</url>
	<ciManagement>
		<system>continuum</system>
		<notifiers>
			<notifier>
				<type>mail</type>
				<configuration>
					<address>rong.acode@gmail.com</address>
				</configuration>
			</notifier>
		</notifiers>
	</ciManagement>
	<developers>
		<developer>
			<id>rong.chen</id>
			<name>Rong Chen</name>
			<email>rong.acode@gmail.com</email>
		</developer>
	</developers>
	<scm>
		<connection>
			scm:svn:http://svn.openehr.org/ref_impl_java/BRANCHES/sandbox/openehr-aom</connection>
		<developerConnection>
			scm:svn:http://svn.openehr.org/ref_impl_java/BRANCHES/sandbox/openehr-aom</developerConnection>
