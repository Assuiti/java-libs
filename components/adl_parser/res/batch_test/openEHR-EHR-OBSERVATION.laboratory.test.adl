archetype
	openEHR-EHR-OBSERVATION.laboratory.test

concept
	[at0000]	-- Laboratory result

definition
	OBSERVATION[at0000] matches {	-- Laboratory result
		data matches {
			HISTORY[at0001] matches {	-- history
				events cardinality matches {1..*} matches {
					EVENT[at0002] occurrences matches {0..*} matches {	-- Any event
						data matches {
							Tree[at0003] matches {	-- Tree
								items cardinality matches {0..1} matches {
									ELEMENT[at0013] occurrences matches {0..1} matches {	-- Diagnostic intervention name
										value matches {
											CODED_TEXT matches {
												code matches {[local::]}	
											}
										}
									}
									ELEMENT[at0012] occurrences matches {0..1} matches {	-- Diagnostic service
										value matches {
											CODED_TEXT matches {
												code matches {
													[local::
													at0015, 	-- Chemistry
													at0016]	-- Microbiology
												}
											}
										}
									}
									CLUSTER[at0004] occurrences matches {0..*} matches {	-- level 1
										runtime_label matches {
											CODED_TEXT matches {
												code matches {[ac0001]}		-- =LOINC::Battery_name
											}
										}
										items cardinality matches {0..1} matches {
											CLUSTER[at0006] occurrences matches {0..*} matches {	-- level 2
												runtime_label matches {
													CODED_TEXT matches {
														code matches {[ac0002]}		-- =LOINC::Battery_name
													}
												}
												items cardinality matches {0..1} matches {
													ELEMENT[at0007] occurrences matches {0..*} matches {*}
													ELEMENT[at0008] occurrences matches {0..1} matches {	-- Comment
														value matches {
															TEXT matches {*}
														}
													}
												}
											}
											ELEMENT[at0005] occurrences matches {0..*} matches {*}
											ELEMENT[at0009] occurrences matches {0..1} matches {	-- Comment
												value matches {
													TEXT matches {*}
												}
											}
										}
									}
									ELEMENT[at0011] occurrences matches {0..*} matches {*}
									ELEMENT[at0010] occurrences matches {0..1} matches {	-- Overall Comment
										value matches {
											TEXT matches {*}
										}
									}
									ELEMENT[at0014] occurrences matches {0..1} matches {	-- Body site
										value matches {
											CODED_TEXT matches {
												code matches {[local::]}	
											}
										}
									}
								}
							}
						}
					}
				}
			}
		}
	}

