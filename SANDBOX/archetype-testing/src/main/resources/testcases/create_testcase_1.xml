<createTestCase>
	<archetype>openEHR-EHR-OBSERVATION.blood_pressure.v1</archetype>
	<tests>	
		<test>
			<inputs>
				<input>
					<path>/data/events[0]/data/items[0]/value/magnitude</path>
					<value>120.0</expected>
				</input>
				<input>
					<path>/data/events[0]/data/items[1]/value/magnitude</path>					
					<value>80.0</value>
				</input>
			</inputs>
			<expected>
				<instance>blood_pressure_001.dadl</instance>
			</expected>			
		</test>		
		<test>
			<inputs>
				<input>
					<path>/data/events[0]/data/items[0]/value/magnitude</path>
					<value>120.0</expected>
				</input>
				<input>
					<path>/data/events[0]/data/items[1]/value/magnitude</path>					
					<value>90.0</value>
				</input>
			</inputs>
			<expected>
				<instance>blood_pressure_002.dadl</instance>
			</expected>			
		</test>
	</tests>
</createTestCase>