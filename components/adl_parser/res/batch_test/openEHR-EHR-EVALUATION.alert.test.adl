archetype
	openEHR-EHR-EVALUATION.alert.test

concept
	[at0000]	-- Alert

definition
	EVALUATION[at0000] matches {	-- Alert
		data matches {
			List[at0003] matches {	-- structure
				items cardinality matches {0..1} matches {
					ELEMENT[at0004] occurrences matches {0..1} matches {	-- Alert
						value matches {
							CODED_TEXT matches {
								code matches {
									[local::
									at0005, 	-- Elite athlete
									at0006, 	-- Pregnancy
									at0007, 	-- Breast feeding
									at0008, 	-- Occupation risk if drowsy
									at0009, 	-- Violent
									at0010, 	-- Previous suicide attempt by overdose
									at0011]	-- Refuses drug products
								}
							}
						}
					}
					ELEMENT[at0012] occurrences matches {0..1} matches {	-- Basis for alert
						value matches {
							CODED_TEXT matches {
								code matches {
									[local::
									at0013, 	-- Health beliefs
									at0014, 	-- Behavioural or occupational
									at0015, 	-- Relgious belief
									at0016, 	-- Patient preference
									at0017]	-- Other
								}
							}
						}
					}
					ELEMENT[at0018] occurrences matches {0..1} matches {	-- Onset date
						value matches {yyyy-??-?? ??:??:??}
					}
					ELEMENT[at0019] occurrences matches {0..1} matches {	-- Age at onset
						value matches {
							REAL_QUANTITY matches {
								property matches {"time"}
								units cardinality matches {1} matches {
									UNIT matches {
										unit_string matches {"yr"}
										magnitude matches {|>= 0|}
									}
								}
							}
						}
					}
					ELEMENT[at0020] occurrences matches {0..1} matches {	-- End date
						value matches {yyyy-??-?? ??:??:??}
					}
				}
			}
		}
	}

